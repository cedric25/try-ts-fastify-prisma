{
  "name": "try-ts-fastify-prisma",
  "version": "0.0.1",
  "private": true,
  "author": "Cedric Nicoloso <cedric.nicoloso@gmail.com>",
  "engines": {
    "node": "=16"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon -r source-map-support/register dist/server.js | pino-pretty -tc",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/server.js",
    "lint": "npm run prettier",
    "prettier": "prettier src admin-scripts --check",
    "prettier:fix": "prettier src admin-scripts --write",
    "eslint": "eslint src --ext .ts",
    "eslint:fix": "eslint src"
  },
  "dependencies": {
    "@hapi/boom": "9.1.4",
    "@prisma/client": "3.12.0",
    "fastify": "3.28.0",
    "fastify-helmet": "7.0.1",
    "fastify-plugin": "3.0.1",
    "fastify-sensible": "3.1.2",
    "module-alias": "2.2.2"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "json-schema-to-ts": "1.6.5",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "prisma": "3.12.0",
    "source-map-support": "0.5.21",
    "typescript": "4.6.3"
  }
}
